<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springmapping.manytomany.CustomerBean"
		name="customer1">
		<property name="id" value="1" />
		<property name="name" value="Suraj" />
		<property name="city" value="Pune" />
		<property name="products">
			<list>
				<bean class="com.jspiders.springmapping.manytomany.ProductBean"
					name="product1">
					<property name="id" value="1" />
					<property name="name" value="shirt" />
					<property name="category" value="dress" />
				</bean>
				<bean class="com.jspiders.springmapping.manytomany.ProductBean"
					name="product2">
					<property name="id" value="2" />
					<property name="name" value="shoes" />
					<property name="category" value="footwear" />
				</bean>
			</list>
		</property>
	</bean>

	<bean class="com.jspiders.springmapping.manytomany.CustomerBean"
		name="customer2">
		<property name="id" value="2" />
		<property name="name" value="Nikhil" />
		<property name="city" value="Mumbai" />
		<property name="products">
			<list>
				<bean class="com.jspiders.springmapping.manytomany.ProductBean"
					name="product1">
					<property name="id" value="1" />
					<property name="name" value="shirt" />
					<property name="category" value="dress" />
				</bean>
				<bean class="com.jspiders.springmapping.manytomany.ProductBean"
					name="product2">
					<property name="id" value="2" />
					<property name="name" value="shoes" />
					<property name="category" value="footwear" />
				</bean>
				<bean class="com.jspiders.springmapping.manytomany.ProductBean"
					name="product3">
					<property name="id" value="3" />
					<property name="name" value="cap" />
					<property name="category" value="dress" />
				</bean>
			</list>
		</property>
	</bean>


</beans>
       