<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
       
       <bean class="com.jspiders.springmapping.onetoone.HusbandBean" >
       <property name="id" value="1"/>
       <property name="name" value="Suraj"/>
       <property name="age" value="23"/>
       <property name="wife" ref="wife1"/>    
       </bean> 
       <bean class="com.jspiders.springmapping.onetoone.WifeBean" name="wife1">
       <property name="id" value="1"/>
       <property name="name" value="Sanika"/>
       <property name="age" value="20"/>
       </bean>
     
        
       </beans>
        