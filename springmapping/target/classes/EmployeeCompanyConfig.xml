<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        <bean class="com.jspiders.springmappingmanytoone.EmployeeBean" name="employee1">
        <property name="id" value="1"/>
        <property name="name" value="Suraj"/>
        <property name="age" value="23"/>
        <property name="company" ref="company1"/>
        
        </bean>
        <bean class="com.jspiders.springmappingmanytoone.EmployeeBean" name="employee2">
        <property name="id" value="2"/>
        <property name="name" value="Nikhil"/>
        <property name="age" value="18"/>
        <property name="company" ref="company1"/>
        
        </bean>
        <bean class="com.jspiders.springmappingmanytoone.CompanyBean" name="company1">
        <property name="id" value="1"/>
        <property name="name" value="Capgemini"/>
        <property name="city" value="Pune"/>
        
        </bean>
        </beans>
       